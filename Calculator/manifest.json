{
   "manifest_version": 2,

   "name": "Calculator",
   "description": "Calculator extension for google chrome. Simple, user-friendly and intuitive.",
   "version": "1.1",

   "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon64.png",
      "128": "icons/icon128.png"
   },
   
   "browser_action": {
      "default_icon": "icons/icon16.png",
      "default_popup": "popup.html"
   },

   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}